#include<stdio.h>
struct student
{
    int age;
    char name[20];
    int marks;
}s[3];
main()
{
    int i;
    for(i=0;i<3;i++)
    {
        printf("enter roll no,name and marks=");
        scanf("%d%s%d",&s[i].age,s[i].name,&s[i].marks);
    }
     for(i=0;i<3;i++)
    {
        printf("-----details of the student---\n");
        printf("age is %d\n",s[i].age);
        printf("name is %s\n",s[i].name);
        printf("marks is %d\n",s[i].marks);
    
     }
}
