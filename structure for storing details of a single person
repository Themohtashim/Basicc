#include<stdio.h>
struct student
{
    int age;
    char name[20];
    int marks;
}s1;
main()
{
    printf("enter age=");
    scanf("%d",&s1.age);
    printf("enter name:");
    scanf("%s",s1.name);
    printf("enter marks:");
    scanf("%d",&s1.marks);
    
    printf("details of student are-\n");
    printf("age is %d\n",s1.age);
    printf("name is %s\n",s1.name);
    printf("marks is %d\n",s1.marks);
}
