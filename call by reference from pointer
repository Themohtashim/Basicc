#include<stdio.h>
void swap(int *x,int *y)
{
    int spy;
    spy=*x;
    *x=*y;
    *y=spy;
}
main()
{
    int a,b;
  printf("enter a=");
  scanf("%d",&a);
  printf("enter b=");
  scanf("%d",&b);
  printf("before swaping\n");
  printf("a=%d,b=%d",a,b);
  swap(&a,&b);
  printf("\nafter swapping\n");
  printf("a=%d,b=%d",a,b);
}
